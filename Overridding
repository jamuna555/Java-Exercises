class Mobile
{
   public void call()
     {
	   System.out.println("we can make voice calls");
	   
	 }
}

class Anroid extends Mobile
{
 public void call()
	{
				 super.call();

		System.out.println("we can make voice and video calls");
	}
}


public class TestAnroid 
{
	public static void main(String args[])
	{
		Anroid a = new Anroid();
		
		a.call();
	}
}
