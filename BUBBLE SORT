import java.util.Arrays;

public class BubbleSort {
	
	public static void main (String args[])
	{
  		int[] a = {10,2,1,-7,110,88,-10};
		
		int temp, i, j;
		
		for(i=0; i<a.length; i++)
		{
			for(j=1; j<a.length; j++)
			{
				if(a[j-1]>a[j])
				{
					temp = a[j-1];
					a[j-1]=a[j];
					a[j]=temp;
				}
			}
		}
	for(i=0; i<a.length; i++)
	{
		System.out.print(a[i]+" ");

	
