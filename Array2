class Marks2
{
	public static void main(String args[])
	{
int[] markList = {55,85,100,65,85}	;
	
	for(int i=0; i<markList.length; i++)
		{
		System.out.println( +markList[i]);
		
		}
					
	 int total = 10;
	 for(int i=0; i<markList.length; i++)
		{
total += markList[i];		
		}
		
		System.out.println( +total);

	}
}
